<externaltool id="copy_file_name">
    <description>Copy current file name</description>
    <displayname>copy file name</displayname>
    <category></category>
    <executable output="ignore" error="ignore" modifiesdocument="no">
        <path>echo</path>
        <arguments>-n %{CurrentDocument:FileName} | /usr/bin/xsel -ib</arguments>
        <workingdirectory>%{CurrentDocument:Path}</workingdirectory>
    </executable>
</externaltool>
